{
  "name": "gatsby-blog",
  "private": true,
  "description": "A blog with gatsby",
  "version": "0.1.0",
  "license": "0BSD",
  "scripts": {
    "build": "env-cmd -f .env gatsby build",
    "develop": "env-cmd -f .env gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "deploy": "env-cmd -f .env gatsby build --prefix-paths && gh-pages -d public",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "dependencies": {
    "@contentful/rich-text-react-renderer": "^14.1.1",
    "gatsby": "^2.24.47",
    "gatsby-plugin-react-helmet": "^3.3.10",
    "gatsby-plugin-sass": "^2.3.12",
    "gatsby-plugin-sharp": "^2.6.30",
    "gatsby-remark-images": "^3.3.28",
    "gatsby-remark-relative-images": "^2.0.2",
    "gatsby-source-contentful": "^2.3.40",
    "gatsby-source-filesystem": "^2.3.25",
    "gatsby-transformer-remark": "^2.8.29",
    "node-sass": "^4.14.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.1.0"
  },
  "devDependencies": {
    "env-cmd": "^10.1.0",
    "gh-pages": "^3.1.0",
    "prettier": "2.0.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-hello-world"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
